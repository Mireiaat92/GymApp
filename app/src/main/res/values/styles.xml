<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <!-- colorPrimary is used for coloring the Toolbar  1.FFA000 2.3F51B5 3.9B26AF 4.009688 -->
        <item name="colorPrimary">#009688</item>
        <!-- colorPrimaryDark is used for coloring the status bar 1.FF8F00 2.303F9F 3.9B26AF 4.009688 -->
